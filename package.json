{
  "name": "@helicone/ai-sdk-provider",
  "version": "1.0.12",
  "description": "Helicone provider for Vercel AI SDK",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "helicone-ai-sdk": "dist/index.js"
  },
  "keywords": [
    "ai",
    "llm",
    "vercel",
    "ai-sdk",
    "helicone",
    "provider",
    "openai",
    "anthropic"
  ],
  "author": "Helicone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Helicone/ai-sdk-provider/"
  },
  "homepage": "https://github.com/Helicone/ai-sdk-provider/",
  "bugs": {
    "url": "https://github.com/Helicone/ai-sdk-provider/issues"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0"
  },
  "devDependencies": {
    "ai": "^5.0.93",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "zod": "^4.0.0"
  },
  "files": [
    "dist"
  ]
}
